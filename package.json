{
  "name": "react-eb",
  "version": "1.0.0",
  "description": "dev/null",
  "main": "dist/server/index.js",
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "build:project": "tsc",
    "build:client": "webpack --config ./webpack.config.js --progress",
    "dev-build:server": "nodemon --config ./nodemon.json -L",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "start": "cross-env NODE_ENV=development run-p dev-build:*",
    "server": "cross-env NODE_ENV=production node dist/server/index.js",
    "clear": "rimraf dist public",
    "prebuild": "npm run clear",
    "prestart": "npm run clear",
    "tslint": "tslint -c ./tslint.json './src/**/*.{ts,tsx}'",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:exec": "docker exec -it typescript-react-express bash"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/lodash": "^4.14.85",
    "@types/node": "^8.0.51",
    "@types/react": "^16.0.21",
    "@types/react-dom": "^16.0.2",
    "@types/react-helmet": "^5.0.3",
    "@types/react-redux": "^5.0.12",
    "@types/react-router": "^4.0.17",
    "@types/react-router-config": "^1.0.5",
    "@types/react-router-dom": "^4.2.1",
    "@types/webpack": "^3.8.1",
    "autoprefixer": "^7.2.2",
    "awesome-typescript-loader": "^3.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "3.0.0-beta.7",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "promise": "^8.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  }
}
